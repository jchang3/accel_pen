#include <math.h>
#include <stdio.h>
#include <stdlib.h>

int main(void) {
	long a;
	a = sqrtl(2500);
	puts("!!!Hello World!!!"); /* prints !!!Hello World!!! */
	printf("%lu", a);
	return EXIT_SUCCESS;
}

